{
  "name": "@edgevector/sdk",
  "version": "0.1.0",
  "description": "Official JavaScript/TypeScript SDK for EdgeVector DB",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "edgevector",
    "database",
    "vector-search",
    "cloudflare",
    "edge",
    "nosql",
    "graphql",
    "sdk"
  ],
  "author": "EdgeVector Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Andrejs1979/EdgeVector"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0"
  },
  "peerDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
